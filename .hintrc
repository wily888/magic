{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "user-select",
          "backface-visibility",
          "scroll-behavior",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink",
          "-webkit-overflow-scrolling",
          "overflow-anchor",
          "user-select: all",
          "-ms-touch-action",
          "-moz-backface-visibility"
        ]
      }
    ],
    "meta-charset-utf-8": "off",
    "axe/name-role-value": "off",
    "disown-opener": "off",
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ],
    "meta-viewport": "off",
    "css-prefix-order": "off"
  }
}